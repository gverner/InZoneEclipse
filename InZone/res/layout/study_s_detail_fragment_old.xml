<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/sdfSymbol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SYM" />

        <TextView
            android:id="@+id/sdfName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/sdfPriceLabel"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/sdfPriceLabel"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfAtr25Label"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/sdfAtr24Label"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfPricePlusAtr25Label"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/sdfPricePlusAtr25Label"
                android:textIsSelectable="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/sdfPrice"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfAtr25"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfPricePlusAtr25"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textIsSelectable="true" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="5dp"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_marginLeft="45dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/sdfUpperSelTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Zone\n " />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#ff0000" />

            <TextView
                android:id="@+id/sdfUpperSelTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:text="WS" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="19dp"
                android:background="#00ff00" />

            <TextView
                android:id="@+id/sdfUpperSelBottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:text="WB" />

            <TextView
                android:id="@+id/sdfUpperBuyTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:text="MA"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfLowerWS"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:text="" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#00ff00" />

            <TextView
                android:id="@+id/sdfLowerWB"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="19dp"
                android:text="" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#ff0000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/sdfColumnHeading3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/sdfEColumnHeader3" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#ff0000" />

            <TextView
                android:id="@+id/sdfWeeklyUpperSellTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfWeeklyUpperSellBottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#00ff00" />

            <TextView
                android:id="@+id/sdfWeeklyUpperBuyTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfMaWeekly"
                style="@style/divider"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfWeeklyLowerSellBottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#00ff00" />

            <TextView
                android:id="@+id/sdfWeeklyLowerBuyTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfWeeklyLowerBuyBottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#ff0000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/sdfColumnHeading4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/sdfEColumnHeader4" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#ff0000" />

            <TextView
                android:id="@+id/sdfMonthlyUpperSellTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfMonthlyUpperSellBottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#00ff00" />

            <TextView
                android:id="@+id/sdfMonthlyUpperBuyTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfMaMonthly"
                style="@style/divider"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfMonthlyLowerSellBottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#00ff00" />

            <TextView
                android:id="@+id/sdfMonthlyLowerBuyTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <TextView
                android:id="@+id/sdfMonthlyLowerBuyBottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textIsSelectable="true" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#ff0000" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:background="#000000" />

</LinearLayout>